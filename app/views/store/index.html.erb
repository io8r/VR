
<h1>Your Catalog</h1>

<% @products.each do |product| %>
	
	<div class="entry">
		<%= image_tag(product.image_url) %>
		<h3><%= product.title %></h3>
		
		<h2><%= product.description %></h2>
		
		<div class="price_line">
			<span class="price"><h3><%= product.price %></h3></span>
		</div>
	</div>

<% end %>



